<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Return">
   <children>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnBackPressed" prefHeight="30.0" prefWidth="78.0" text="Back" />
      <Label layoutX="212.0" layoutY="14.0" text="Return book">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Button fx:id="btnConfirm" layoutX="270.0" layoutY="200.0" mnemonicParsing="false" onAction="#btnConfirmPressed" text="Confirm" />
      <ComboBox fx:id="cmbList" layoutX="224.0" layoutY="135.0" onShowing="#cmbListPressed" prefWidth="150.0" />
      <TextField fx:id="txtuserID" disable="true" editable="false" layoutX="226.0" layoutY="73.0" />
   </children>
</AnchorPane>
