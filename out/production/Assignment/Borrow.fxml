      <Button fx:id="btnConfirm" layoutX="252.0" layoutY="251.0" mnemonicParsing="false" onAction="#btnConfirmPressed" prefHeight="30.0" prefWidth="96.0" text="Confirm" />
      <Label layoutX="213.0" layoutY="6.0" text="Borrow loan">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Label layoutX="321.0" layoutY="99.0" text="Date start:" />
      <Label layoutX="322.0" layoutY="183.0" text="Date end:" />
      <ComboBox fx:id="cmbList" layoutX="92.0" layoutY="179.0" onShowing="#cmbListPressed" prefWidth="150.0" />
      <TextField fx:id="txtuserID" disable="true" editable="false" layoutX="100.0" layoutY="94.0" prefHeight="26.0" prefWidth="134.0" />
      <TextField fx:id="txtdateStart" disable="true" editable="false" layoutX="397.0" layoutY="95.0" />
      <TextField fx:id="txtdateEnd" disable="true" editable="false" layoutX="397.0" layoutY="179.0" />
   </children>
</AnchorPane>
